{
  "name": "burni-website",
  "version": "1.0.0",
  "scripts": {
    "dev:css": "tailwindcss -i ./src/styles.css -o ./assets/css/styles.min.css --watch",
    "build:css": "cross-env NODE_ENV=production tailwindcss -i ./src/styles.css -o ./assets/css/styles.min.css --minify",
    "prebuild": "npx update-browserslist-db@latest",
    "build": "npm run build:css",
    "postbuild": "echo Build completed successfully!"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.25.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17"
  },
  "homepage": "https://burnitoken.com/",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "files": [
    "src/",
    "assets/css/styles.min.css",
    "package.json",
    "tailwind.config.js"
  ]
}
